page {
  background-color: #f8f8f8;
}
.title_wrap {
  background-color: #ffffff;
  padding: 10rpx 10rpx;
  border-left: 10rpx solid #faae58;
  font-size: 36rpx;
  color: #353535;
  margin: 10rpx 10rpx;
}
.documents_wrap .my_document {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 10rpx;
  border: 1rpx solid #cccccc;
  border-radius: 20rpx;
  margin: 20rpx 10rpx;
  background-color: #ffffff;
}
.documents_wrap .my_document navigator {
  flex: 5;
  display: flex;
  justify-content: center;
  align-items: center;
}
.documents_wrap .my_document navigator .document_picture {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.documents_wrap .my_document navigator .document_picture image {
  width: 135rpx;
  height: 170rpx;
  border-radius: 20rpx;
}
.documents_wrap .my_document navigator .name_and_introduction_and_score {
  flex: 5;
  padding-right: 10rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: flex-start;
  height: 170rpx;
}
.documents_wrap .my_document navigator .name_and_introduction_and_score .name {
  flex: 1;
  color: #faae58;
  font-size: 36rpx;
}
.documents_wrap .my_document navigator .name_and_introduction_and_score .introduction {
  flex: 2.4;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #353535;
  font-size: 28rpx;
}
.documents_wrap .my_document navigator .name_and_introduction_and_score .score {
  flex: 1;
  font-size: 30rpx;
  color: #353535;
}
.documents_wrap .my_document .collections_and_delete {
  flex: 1.5;
}
.documents_wrap .my_document .collections_and_delete .common {
  display: flex;
  justify-content: center;
  align-items: center;
}
.documents_wrap .my_document .collections_and_delete .common .iconfont {
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: #353535;
  padding-right: 5rpx;
}
.documents_wrap .my_document .collections_and_delete .common .detail {
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 26rpx;
  color: #353535;
}
