.view_title {
  width: 100%;
  text-align: center;
  padding: 10rpx 0;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #cccccc;
}
.view_title .view_name {
  color: #faae58;
  font-size: 20px;
}
.gender_wrap {
  display: inline-flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  line-height: 48px;
  height: 48px;
}
.gender_wrap .xing {
  width: 16px;
  font-size: 14px;
  text-align: right;
  color: #ee0a24;
}
.gender_wrap .gender {
  width: 64px;
  text-align: left;
  color: #646566;
  margin-right: 12px;
}
.gender_wrap .gender_van {
  display: flex;
  justify-content: center;
  align-items: center;
}
.submit_wrap {
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  margin-top: 50rpx;
}
.common_wrap {
  padding: 10rpx 0;
  padding-left: 10rpx;
}
.birthday_wrap {
  display: inline-flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  line-height: 48px;
  height: 48px;
}
.birthday_wrap .xing {
  width: 16px;
  font-size: 14px;
  text-align: right;
  color: #ee0a24;
}
.birthday_wrap .birthday {
  width: 64px;
  text-align: left;
  color: #646566;
  margin-right: 12px;
}
.birthday_wrap .birthday_van {
  color: #c5c5c5;
}
.birthday_wrap .active_birthday_van {
  color: #353535;
}
